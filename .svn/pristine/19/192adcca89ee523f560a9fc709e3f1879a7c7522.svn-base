  	<!-- 결재 완료 문서함 페이징 -->
  	<select id="selectCompleteCount" parameterType="kr.or.ddit.comm.paging.vo.PaginationInfoVO" resultType="int">
  		SELECT COUNT (*) total_count
		FROM (
		    SELECT *
