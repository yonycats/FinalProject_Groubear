<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="kr.or.ddit.company.companyInfo.mapper.IInfoMapper">
  	<select id="getContract" parameterType="kr.or.ddit.comm.security.vo.CompanyVO" resultType="kr.or.ddit.groubear.contract.vo.ContractVO">
  		SELECT * 
  		FROM
  			CONTRACT
  		WHERE
  			CO_CD = #{coCd}
  	</select>
  
  </mapper>